<nav>
	<ul><p><%= link_to 'Home', root_path %></p></ul>
	<ul><p><%= link_to 'About', about_index_path %></p></ul>
</nav>

<%= form_tag url_for(:controller => 'providers', :action => 'index'), :method => 'get' do %>
	<%= text_field_tag :search %>
