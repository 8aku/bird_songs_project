<nav>
	<ul><p><%= link_to 'Home', root_path %></p></ul>
	<ul><p><%= link_to 'About', about_index_path %></p></ul>
</nav>

<%= form_tag url_for(:controller => 'countries', :action => 'index'), :method => 'get' do %>
	<%= text_field_tag :search %>
  <%= submit_tag 'Search', name: nil %>
<% end %>

<% @countries.each do |country| %>
<h2><%= country.country %></h2>
<% end %>

<%= paginate @countries %> 